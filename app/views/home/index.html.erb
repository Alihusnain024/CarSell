
<div class="homepage">
<% if user_signed_in? %>
  <%= render 'shared/navbar' %> 
  <div class="home-container">
<div>
<div class="row2">
<%= select_tag :carModel, options_for_select([["suzuki", "suzuki"], ["toyota", "toyota"], ["honda", "honda"]]), include_blank: "Select Car Model" %>
  <%= select_tag :city, options_for_select([["rawalpindi", "rawalpindi"], ["lahore", "lahore"], ["karachi", "karachi"], ["peshawar","peshawar"], ["islamabad","islamabad"]]), include_blank: "All Cities"  %>
  <%= select_tag :price, options_for_select([["1000000", "price1"], ["2000000", "price2"], ["3000000", "price3"]]), include_blank: "Price range" %>
</div>
<div class="row2">
  <%= select_tag :engineType, options_for_select([["petrol", "petrol"], ["diesel", "diesel"], ["hybrid", "hybrid"]]),include_blank: "Engine Details"  %>
  <%= select_tag :transmissonType, options_for_select([["manual", "manual"], ["automatic", "automatic"]]), include_blank: "Transmission Type" %>
  <%= select_tag :color, options_for_select([["black", "black"], ["white", "white"], ["gray", "gray"], ["silver", "silver"], ["red","red"]]),include_blank: "Color" %>
</div>
<div class="row2">
  <%= select_tag :mileage, options_for_select([["10", "mileage1"], ["15", "mileage2"], ["20", "mileage3"]]) , include_blank: "mileage"%>
  <%= select_tag :engineCapicity, options_for_select([["1.0L", "e1"], ["2.0L", "e2"], ["3.0L", "e3"]]), include_blank: "engine Capacity" %>
  <%= select_tag :assemblyType, options_for_select([["local", "local"], ["imported", "imported"]]), include_blank: "Assembly Type" %>
</div>
<div class='d-flex justify-content-end'>
    <button type="button" class="btn btn-primary m-1">Search</button>
    <button type="button" class="btn btn-secondary m-1">Clear Filters</button>
    </div>
</div>
</div>
<% else %>
  <p>Please <%= link_to "Sign up", new_user_registration_path, :class => 'navbar-link' %> or <%=link_to "Login", new_user_session_path, :class => 'navbar-link' %></p>
<% end %>

<div class="list-container">
<div class="card">
  <div class="card-body">
  <div class="d-flex" >
    <img src="data:image/jpeg;base64," alt="Image Description" class="float-left mr-3" width="100" height="100">
    <div class="card-info" >
    <div class="d-flex justify-content-between">
    <h5 class="card-title">Suzuki Wagon R</h5>
    <p class="card-text">PKR 15.7 Lakh</p>
    </div>
    <p class="card-text">islambad</p>

    <a href="#" class="btn btn-primary phone-button ">Show Phone No</a>

    </div>
    </div>
  </div>
  </div>
</div>


</div>





